{
  "name": "jose-chan/pb4php",
  "description": "protocol buffer for php",
  "keywords": ["protobuf", "Protocol Buffer"],
  "authors": [{
    "name": "JoseChan",
    "email": "524233828@qq.com"
  }],
  "license": "MIT",
  "type": "library",
  "require": {
    "php": "^5.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "JoseChan\\ProtoBuf\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
